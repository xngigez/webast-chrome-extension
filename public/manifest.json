{
	"name": "WebAst",
	"description": "AI browsing assistant.",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "index.html",
		"default_title": "Open the popup"
	},
	"icons": {
		"512": "logo512.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"scripts/content.js"
			]
		}
	],
	"permissions": [
		"activeTab",
		"storage"
	]
}